{
  "expo": {
    "name": "MindbodyFit",
    "slug": "MindbodyFit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Mindbody-Fit.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Mindbody-Fit.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindbody.MindbodyFit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Mindbody-Fit.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mindbody.MindbodyFit"
    },
    "web": {
      "favicon": "./assets/images/Mindbody-Fit.png"
    },
    "description": "MindbodyFit Project"
  }
}
